[Package]
name = "iptables"
version = "1.8.10"
description = "Linux kernel packet filtering framework"
url = "https://netfilter.org/projects/iptables/files/iptables-1.8.10.tar.xz"
category = "network"

[Dependencies]
depends = ""

[Build]
configure_flags = "--prefix=/usr --sysconfdir=/etc"
make_flags = "-j$(nproc)"
install_target = "install"

[Script]
./configure --prefix=/usr --sysconfdir=/etc --enable-libipq
make -j$(nproc)
make install
