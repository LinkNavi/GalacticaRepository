[Package]
name = "tar"
version = "1.35"
description = "GNU tape archiver"
url = "https://ftp.gnu.org/gnu/tar/tar-1.35.tar.xz"
category = "system"

[Dependencies]
depends = ""

[Build]
configure_flags = "--prefix=/usr"
make_flags = "-j$(nproc)"
install_target = "install"

[Script]
./configure --prefix=/usr
make -j$(nproc)
make install
