[Package]
name = "libunistring"
version = "1.1"
description = "Unicode string library"
url = "https://ftp.gnu.org/gnu/libunistring/libunistring-1.1.tar.xz"
category = "libraries"

[Dependencies]
depends = ""

[Build]
configure_flags = "--prefix=/usr"
make_flags = "-j$(nproc)"
install_target = "install"

[Script]
./configure --prefix=/usr
make -j$(nproc)
make install
